{
  "name": "staking-hub-api",
  "version": "0.0.1",
  "description": "backend server for staking hub",
  "type": "commonjs",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/memory-cache": "^0.2.6",
    "@types/node": "^16.18.119",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "deterministic-object-hash": "^2.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "graphql-request": "6.1.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "merkletreejs": "^0.5.1",
    "openai": "^4.71.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.14",
    "siwe": "^3.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3",
    "viem": "^2.23.5",
    "winston": "^3.16.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint --fix --max-warnings=0",
    "prettier": "prettier \"src/**/*.{ts,json,md}\"",
    "format": "prettier \"src/**/*.{ts,json,md}\" --write",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "generate": "npm run typeorm -- migration:generate -d src/data-source.ts src/migration/$npm_config_name",
    "generate:custom": "npm run typeorm -- migration:create src/migration/$npm_config_name",
    "migrate": "npm run typeorm -- migration:run -d src/data-source.ts",
    "migrate:fake": "npm run typeorm -- migration:run -d src/data-source.ts --fake",
    "revert": "npm run typeorm -- migration:revert -d src/data-source.ts",
    "revert:fake": "npm run typeorm -- migration:revert -d src/data-source.ts --fake",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/!(index).ts": [
      "eslint --fix --max-warnings=0",
      "prettier \"src/**/*.{ts,json,md}\" --write"
    ]
  }
}
